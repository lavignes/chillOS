pkg UInt;

use U64;

pub let MAX: UInt = U64::MAX as UInt;
pub let MIN: UInt = U64::MIN as UInt;
pub let BITS: UInt = U64::BITS;

pub let ERR_OUT_OF_RANGE: Int = -1;
pub let ERR_MALFORMED: Int = -2;

pub fn parse(into: &mut UInt, base: UInt, str: &U8, len: UInt): Int {
    return U64::parse(into as &mut U64, base, str, len);
}
