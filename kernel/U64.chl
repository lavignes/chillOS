pkg U64;

use Ascii;
use UInt;

pub let MAX: U64 = 0xFFFF_FFFF_FFFF_FFFFu64;
pub let MIN: U64 = 0x0000_0000_0000_0000u64;
pub let BITS: U64 = sizeof U64 * 8;

pub fn parse(into: &mut U64, base: UInt, str: &U8, len: UInt): Int {
    let mut tmp: UInt;
    let ret: Int = UInt::parse(&mut tmp, base, str, len);
    if ret != 0 {
        return ret;
    }
    if tmp > (MAX as UInt) {
        return UInt::ERR_OUT_OF_RANGE;
    }
    *into = tmp as U64;
    return 0;
}
