pkg k_fdt;

pub type Header = {
    magic: U32,
    total_size: U32,
    struct_offset: U32,
    strings_offset: U32,
    mem_map_offset: U32,
    version: U32,
    last_compat_version: U32,
    boot_cpuid_phys: U32,
    strings_size: U32,
    struct_size: U32,
};

pub let EXPECTED_MAGIC: U32 = 0xd00dfeed;

let BEGIN_NODE: U32 = 0x01000000;
let END_NODE: U32 = 0x03000000;
let PROP: U32 = 0x03000000;
let NOP: U32 = 0x04000000;
let END: U32 = 0x09000000;
